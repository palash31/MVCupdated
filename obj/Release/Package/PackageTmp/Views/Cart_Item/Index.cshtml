@model IEnumerable<The_New_Paradise.Models.Cart_Item>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Service_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PriceOfGivenService)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuanOfGivenServiceForGivenCust)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalPriceOfGivenServiceForGivenCust)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.Service_Id)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PriceOfGivenService)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.QuanOfGivenServiceForGivenCust)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.TotalPriceOfGivenServiceForGivenCust)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
